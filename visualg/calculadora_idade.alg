algoritmo "QUAL_SUA_IDADE"
// Função :
// Autor :
// Data : 17/05/2022
// Seção de Declarações 
var
        ano_nasc, ano_atual, idade: inteiro
inicio
      escreva ("em que ano nós estamos?")
      leia (ano_atual)
      escreva ("em que ano você nasceu?")
      leia (ano_nasc)
      idade <- ano_atual - ano_nasc
      escreva ("sua idade é ", idade)
      
      
fimalgoritmo
